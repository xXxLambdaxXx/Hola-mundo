#include<iostream>

using namespace std;

int main()
{
 int a,b,c;
 cout<<"Dame dos numeros: "<<endl;
 cin>>a;
 cin>>b;
 c=a+b;
 cout<<"El resultado de la suma es: "<<c<<endl;
 c=a-b;
 cout<<"El resultado de la resta es: "<<c<<endl;
 c=a*b;
 cout<<"El resultado de la multiplicacion es: "<<c<<endl;
 c=a/b;
 cout<<"El resultado de la division es: "<<c<<endl;
 return 0;
}
